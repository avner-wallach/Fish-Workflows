        <scr:Script>import time
import os
from datetime import date
today = date.today()
@returns(str)
def process(value):
  sep="\\"
  month='{0:d}'.format(today.month)
  d='{:%Y%m%d}'.format(today)
  path=value+d+sep
  if not os.path.exists(path):
    os.mkdir(path)

  fname = path+'counter.txt'
  if not os.path.isfile(str(fname)):
    file= open(fname,'w') 
    file.write(str(0))
    file.close()


  return path</scr:Script>
